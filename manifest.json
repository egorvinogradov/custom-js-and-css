{
    "name": "Custom JS & CSS",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "page": "background.html"
    },
    "options_page": "options.html"
}
